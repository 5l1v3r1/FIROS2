<?xml version="1.0"?>
<is>
    <profiles>
        <participant profile_name="rtps">
            <rtps>
                <builtin>
                    <domainId>0</domainId>
                </builtin>
            </rtps>

            <subscriber profile_name="fastrtps_subscriber">
                <topic>
                    <name>HelloWorldTopic</name>
                    <dataType>HelloWorld</dataType>
                </topic>
                <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            </subscriber>
        </participant>

        <participant profile_name="ros2">
            <rtps>
                <builtin>
                    <domainId>0</domainId>
                </builtin>
            </rtps>

            <publisher profile_name="ros2_publisher">
                <topic>
                    <name>chatter</name>
                    <dataType>std_msgs::msg::dds_::String_</dataType>
                </topic>
                <qos>
                    <partition>
                        <names>
                            <name>rt</name>
                        </names>
                    </partition>
                </qos>
                <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            </publisher>
        </participant>
    </profiles>

    <connector name="domain_change">
        <subscriber participant_name="rtps" subscriber_name="fastrtps_subscriber"/>
        <publisher participant_name="ros2" publisher_name="ros2_publisher"/>
        <transformation file="C:/git/ros2/src/ros2/firos2/examples/helloworld/build/Debug/userlib.dll" function="transform"/>
    </connector>
</is>
