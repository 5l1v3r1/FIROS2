<?xml version="1.0"?>
<is>
    <topic_types>
        <type name="HelloWorld">
            <participants>
                <participant name="rtps"/>
            </participants>
        </type>
        <type name="std_msgs::msg::dds_::String_">
            <participants>
                <participant name="ros2"/>
            </participants>
        </type>
    </topic_types>

    <profiles>
        <transports>
            <transport>
                <transport_id>TCPv4_Server</transport_id>
                <type>TCPv4</type>
                <wan_addr>127.0.0.1</wan_addr>
                <listening_ports>
                    <port>5100</port>
                </listening_ports>
                <metadata_logical_port>7402</metadata_logical_port>
            </transport>
            <transport>
                <transport_id>TCPv4_Client</transport_id>
                <type>TCPv4</type>
                <metadata_logical_port>7403</metadata_logical_port>
            </transport>
        </transports>

        <participant profile_name="rtps">
            <rtps>
                <userTransports>
                    <transport_id>TCPv4_Client</transport_id>
                </userTransports>   
                <defaultUnicastLocatorList>
                    <locator>
                        <kind>TCPv4</kind>
                        <address>127.0.0.1</address>
                        <ports_>
                            <physical_port>5100</physical_port>
                            <logical_port>7411</logical_port>
                        </ports_>
                    </locator>
                </defaultUnicastLocatorList>
                <builtin>
                    <domainId>0</domainId>
                    <leaseDuration>
                        <durationbyname>INFINITE</durationbyname>
                    </leaseDuration>
                    <leaseAnnouncement>
                        <durationbyval>
                            <seconds>0</seconds>
                            <fraction>333</fraction>
                        </durationbyval>
                    </leaseAnnouncement>
                    <metatrafficUnicastLocatorList>
                        <locator>
                            <kind>TCPv4</kind>
                            <address>127.0.0.1</address>
                            <ports_>
                                <physical_port>5100</physical_port>
                                <logical_port>7403</logical_port>
                            </ports_>
                        </locator>
                    </metatrafficUnicastLocatorList>
                    <initialPeersList>
                        <locator>
                            <kind>TCPv4</kind>
                            <address>127.0.0.1</address>
                            <ports_>
                                <physical_port>5100</physical_port>
                                <logical_port>7402</logical_port>
                            </ports_>
                        </locator>
                        <locator>
                            <kind>TCPv4</kind>
                            <address>127.0.0.1</address>
                            <ports_>
                                <physical_port>5100</physical_port>
                                <logical_port>7410</logical_port>
                            </ports_>
                        </locator>
                    </initialPeersList>
                </builtin>
                <use_IP4_to_send>true</use_IP4_to_send>
                <use_IP6_to_send>false</use_IP6_to_send>
                <useBuiltinTransports>false</useBuiltinTransports>
